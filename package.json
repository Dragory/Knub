{
  "name": "knub",
  "version": "30.0.0-beta.37",
  "description": "A bot framework for Discord",
  "author": "Miikka Virtanen <contact@mivir.fi>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --exit -r ts-node/register src/**/*.test.ts -- --trace-warnings",
    "lint": "eslint src",
    "build": "shx touch dist && shx rm -r dist && tsc --sourceMap false && shx cp .npmignore dist/",
    "preversion": "npm run test",
    "prepublishOnly": "npm run test && npm run build",
    "watch": "shx touch dist && shx rm -r dist && tsc --watch",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.10",
    "knub-command-manager": "^9.1.0",
    "node-fetch": "^2.6.1",
    "ts-essentials": "^6.0.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.45",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "eslint": "^7.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "prettier": "^2.3.0",
    "shx": "^0.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "eris": "^0.15.1"
  },
  "files": [
    "/dist"
  ]
}
